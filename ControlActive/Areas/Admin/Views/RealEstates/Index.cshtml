                                </select>
                            </div>
                            <div class="col-sm-1">
                                <label style="font-size:11px;" class="col-sm-12 col-form-label">@Localizer["OnBalance"]</label>

                                <div class="media-body text-start icon-state">
                                    <label class="switch">
                                        <input id="balance_" name="balance" type="checkbox" checked><span class="switch-state"></span>
                                    </label>
                                </div>

                            </div>
                            <div class="col-sm-2">
                                <label style="font-size:11px;" class="col-sm-12 col-form-label">@Localizer["NotOnBalance"]</label>

                                <div class="media-body text-start icon-state">
                                    <label class="switch">
                                        <input id="notBalance_" name="notBalance" type="checkbox" checked><span class="switch-state"></span>
                                    </label>
                                </div>

                            </div>
                            <div class="col-sm-3">
                                <label class="col-form-label">Вилоятни танланг</label>
                                <select asp-items=@ViewBag.Regions name="region" id="region_" class="js-example-basic-hide-search col-sm-6">
                                    <option selected value="0"> -Барчаси-</option>
                                </select>
                            </div>
                            <div class="col-sm-2">
                                <label class="col-form-label">Санани танланг</label>
                                <input value="@date" name="chosenDate" id="chosenDate" class="datepicker-here form-control digits input-air-primary" />


                            </div>

                        </div>
                    </div>
                    <hr class="mt-4 mb-4">
                    <!-- Case for all assets -->
                    <div id="case1">

