@model IEnumerable<ControlActive.Models.RealEstate>
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@{

    ViewData["Title"] = "Кўчмас мулк объектлари";
    var dateAndTime = DateTime.Now;
    var date = dateAndTime.ToShortDateString();



}

<style>

.file-icon{
        transition: transform .5s;
}
.file-icon:hover{
        transform: scale(1.5);
}

    .deleteC:hover {
        color: red;
    }

    .editC:hover {
        color: #fcba03;
    }

    .passC:hover {
        color: #3809b8
    }

    .filesC:hover {
        color: #c91697
    }

    .detailC:hover {
        color: #02cbcf
    }

    .userC:hover {
        color: #56c700
    }

    .loader-w {
        position: fixed;
        left: 0px;
        top: 40%;
        width: 100%;
        height: 100%;
        z-index: 9999;
    }

</style>


<div id="loader" class="loader-w" hidden>

    <div  class="loader-box">
            <div style="font-size:30px; " class="loader-10"></div>
        </div>
   
</div>

<div class="container-fluid">
    <div class="page-title">
        <div class="row">
            <div class="col-12 col-sm-6">
                <h3>Умумий маълумотлар<small>(кўчмас мулк объектлари)</small></h3>
            </div>
            <div class="col-12 col-sm-6">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href=""><i data-feather="home"></i></a></li>
                    <li class="breadcrumb-item"> </li>
                    <li class="breadcrumb-item active"></li>
                </ol>
            </div>
        </div>
    </div>


</div>


<div class="container-fluid">

    <div class="row">
        <div class="col-sm-12 col-md-12 col-xl-12">
            <div class="card">
                <div class="card-header b-l-primary border-3 ">
                    <div class="row">

                        <div class="col-sm-4">
                            <label class="col-form-label">Ташкилотни танланг</label>
                            <select asp-items=@ViewBag.Organizations name="org" id="org" class="js-example-basic-hide-search col-sm-6">
                                <option selected value="0"> -Барчаси-</option>
                            </select>
                        </div>
                        <div class="col-sm-1">
                            <label class="col-sm-12 col-form-label">@Localizer["OnBalance"]</label>

                            <div class="media-body text-start icon-state">
                                <label class="switch">
                                    <input id="balance" name="balance" type="checkbox" checked><span class="switch-state"></span>
                                </label>
                            </div>

                        </div>
                        <div class="col-sm-2">
                            <label class="col-sm-12 col-form-label">@Localizer["NotOnBalance"]</label>

                            <div class="media-body text-start icon-state">
                                <label class="switch">
                                    <input id="notBalance" name="notBalance" type="checkbox" checked><span class="switch-state"></span>
                                </label>
                            </div>

                        </div>
                        <div class="col-sm-3">
                            <label class="col-form-label">Вилоятни танланг</label>
                            <select asp-items=@ViewBag.Regions name="region" id="region" class="js-example-basic-hide-search col-sm-6">
                                <option selected value="0"> -Барчаси-</option>
                            </select>
                        </div>
                        <div class="col-sm-2">
                            <div style="margin-top:35px;">
                                <span data-bs-toggle="modal" data-bs-target="#reportModal"><button data-bs-toggle="tooltip" data-bs-placement="top" title="" data-bs-original-title="ҳисобот олиш" class="btn btn-outline-success"><i class="icofont icofont-flash"></i><i class="icofont icofont-table"></i></button></span>
                            </div>


                        </div>

                    </div>


                </div>
                <div class="card-body">
                    <div class="dt-ext table-responsive" id="table1">

                        <table id="myTable" class="table display dt-responsive " style="width:100%">
                            <thead>
                                <tr>
                                    <th style="font-size:12px">
                                        Объект номи @*<a hidden id="bulkDelete" data-bs-toggle="tooltip" data-bs-placement="top" data-bs-original-title="Танланган ташкилот бўйича барчасини ўчириш" onclick="DeleteByOrganization(document.getElementById('org').options[document.getElementById('org').selectedIndex].value, document.getElementById('org').options[document.getElementById('org').selectedIndex].text)" class="deleteC" style="cursor:pointer; width:120px">
                                            <i style="color:black" class="fa fa-long-arrow-right"></i><i style="color:black" class="fas fa-trash-alt"></i>  &nbsp &nbsp
                                        </a>*@
                                    </th>
                                    <th style="font-size:12px">Кадастр рақами</th>
                                    <th style="font-size:12px">Ишга тушириш санаси</th>
                                    <th style="font-size:12px">Фаолият тури</th>
                                    <th style="font-size:12px">Вилоят</th>
                                    <th style="font-size:12px">Баланс сақловчи ташкилот</th>
                            
                                    <th style="text-align:center"><i class="fa fa-gears"></i></th>

                                </tr>
                            </thead>


                        </table>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade modal-bookmark" id="reportModal" tabindex="-1" role="dialog" aria-labelledby="reportModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="createModalLabel">@Localizer["CreateReport"]</h5>
                <button class="btn-close" type="button" data-bs-dismiss="modal" aria-label="Close">                                                 </button>
            </div>
            <div class="modal-body">
